##########################################
#           玩家机器人配置文件           #
##########################################

# 启用或禁用 AI 玩家机器人
AiPlayerbot.Enabled = 1

# 启用随机机器人系统
AiPlayerbot.RandomBotAutologin = 1

# 启动时登录所有随机机器人
AiPlayerbot.RandomBotLoginAtStartup = 1

# 删除所有随机机器人账号（重置随机机器人）
AiPlayerbot.DeleteRandomBotAccounts = 0

# 玩家登录时自动登录所有玩家角色（作为机器人）
AiPlayerbot.BotAutologin = 0

# 公会任务系统
AiPlayerbot.EnableGuildTasks = 0

# 为随机机器人启用LFG寻求组队
AiPlayerbot.RandomBotJoinLfg = 1

# 为随机机器人启用地下城建议
AiPlayerbot.RandomBotSuggestDungeons = 1

# 随机以小写字母启用地下城建议Enable dungeon suggestions in lower case randomly
AiPlayerbot.SuggestDungeonsInLowerCaseRandomly = 0

# 为机器人启用战场/竞技场
AiPlayerbot.RandomBotJoinBG = 1

# 启用自动加入战场 - 如果服务器没有延迟，机器人随机加入战歌峡谷和2v2竞技场
AiPlayerbot.RandomBotAutoJoinBG = 0

# 将任务等级 <= 机器人等级的任务标记为已完成（减慢机器人创建速度）
AiPlayerbot.PreQuests = 0

# 随机机器人账号
AiPlayerbot.MinRandomBots = 500
AiPlayerbot.MaxRandomBots = 500
AiPlayerbot.RandomBotMinLevel = 1
AiPlayerbot.RandomBotMaxLevel = 70

# 启用/禁用随机机器人轮换 (从机器人角色库中随机选择一个机器人上限并定期轮换)
# 改变设置后需重置随机机器人
# 默认: 0 (禁用，在线机器人是固定的)
AiPlayerbot.EnableRotation = 0

# 用于轮换的机器人库角色数量 (需要少于RandomBotAccountCount * 10)
AiPlayerbot.RotationPoolSize = 500

# 用于创建随机机器人的账号
AiPlayerbot.RandomBotAccountPrefix = "rndbot"
AiPlayerbot.RandomBotAccountCount = 1000

# 随机机器人公会数量
AiPlayerbot.RandomBotGuildCount = 20

# 删除所有随机机器人公会
AiPlayerbot.DeleteRandomBotGuilds = 0

# 随机机器人竞技场队伍数量
AiPlayerbot.RandomBotArenaTeamCount = 20

# 删除所有随机机器人竞技场队伍
AiPlayerbot.DeleteRandomBotArenaTeams = 0

# 低装备等级随机机器人概率
AiPlayerbot.RandomGearLoweringChance = 0

# 第一次随机化时随机机器人拥有最大等级的概率 (默认 0.15)
AiPlayerbot.RandomBotMaxLevelChance = 0.15

# 机器人选择 RPG (那他们等级传动到随机营地)而不是grinding的概率
AiPlayerbot.RandomBotRpgChance = 0.20 #unused now

# Set randombots movement speed to walking anywhere
AiPlayerbot.RandombotsWalkingRPG = 0

# Set randombots movement speed to walking only inside buildings
AiPlayerbot.RandombotsWalkingRPG.InDoors = 0

# 机器人向玩家致意
AiPlayerbot.EnableGreet = 0

# 机器人接受邀请时将被传送到玩家身边
AiPlayerbot.SummonWhenGroup = 1

# 在机器人身上显示头盔和披风 (需要重置)
AiPlayerbot.RandomBotShowHelmet = 1
AiPlayerbot.RandomBotShowCloak = 1

# 禁用随机机器人随机等级
# 每一个机器人都从指定的级别开始，并通过杀死暴徒来升级。
AiPlayerbot.DisableRandomLevels = 0

# 设置随机机器人起始等级，如果 "AiPlayerbot.DisableRandomLevels" 启用
AiPlayerbot.RandombotStartingLevel = 5

# 设置机器人击杀获得经验倍率 (默认: 1)
# Server XP Rate * AiPlayerbot.KillXPRate
AiPlayerbot.KillXPRate = 1

# Specify percent of active bots
# 默认是10。每分钟有10%的机器人处于活动或非活动状态。
AiPlayerbot.BotActiveAlone = 100

# 设置随机机器人获得装备时附魔的最低等级 (Maxlevel + 1 to disable)
# 默认: 60
AiPlayerbot.MinEnchantingBotLevel = 60

# 随机机器人检查玩家装备GS等级，如果太低则拒绝邀请
# 默认: 1 (启用)
AiPlayerbot.GearScoreCheck = 0

# 将完成并奖励给所有随机机器人的任务
AiPlayerbot.RandomBotQuestIds = "7848,3802,5505,6502,7761"

# 随机机器人将与附近的机器人组队去做共有任务
AiPlayerbot.RandomBotGroupNearby = 0

# 没有主人的机器人会说出他们的台词
AiPlayerbot.RandomBotSayWithoutMaster = 0

# Set RandomBotMaxLevel bots to RandomBotMinLevel or not
AiPlayerbot.DowngradeMaxLevelBot = 0

# 启用/禁用 达到EquipmentPersistenceLevel设置等级后，机器人装备不再随机初始化 
# 默认：0 (禁用)
AiPlayerbot.EquipmentPersistence = 0

# 默认：80
AiPlayerbot.EquipmentPersistenceLevel = 80

# 机器人队伍邀请权限级别 (0 = 只有GM， 1 = 基于等级接受， 2 = 总是接受)
# 默认：1 (基于等级接受)
AiPlayerbot.GroupInvitationPermission = 1

# 启用/禁用 机器人被召唤时将复活并修理装备 (0 = 从不，1 = 非战斗且存活时启用, 2 = 总是启用)
# 默认：1 (非战斗启用)
AiPlayerbot.BotReviveWhenSummon = 1

# Non-GM player can only use init=auto to initialize bots based on their own level and gear score
# 默认：0 (non-gm player can use any intialization commands)
AiPlayerbot.AutoInitOnly = 0

# The upper limit ratio of bot equipment level for init=auto
# 默认：1.0 (same with the player)
AiPlayerbot.AutoInitEquipLevelLimitRatio = 1.0

# 启用/禁用 通过addclass命令创建机器人(0 = 只有GM ，1 = 启用)
# 默认：1 (启用)
AiPlayerbot.AddClassCommand = 1


# 自动化

# Bots keep looting when group loop method is free for all
# 默认：0 (禁用)
AiPlayerbot.FreeMethodLoot = 0

# 机器人选择任务奖励 (yes = 选择第一个可用物品，no = 列出所有奖励，ask = 选择可用物品并列出如果多个)
AiPlayerbot.AutoPickReward = no

# 机器人装备升级 (机器人将装备任何拾取或任务获取的有提升的物品)
# 默认：1 (启用)
AiPlayerbot.AutoEquipUpgradeLoot = 1

# Equip upgrade threshold for auto equip upgrade
# 默认：1.1 (Equip when the equipment score is 1.1 times higher than the current)
AiPlayerbot.EquipUpgradeThreshold = 1.1

# 与玩家同步任务 (当你提交任务时机器人将完成任务。机器人将忽略任务物品需求。)
# 默认：1 (启用)
AiPlayerbot.SyncQuestWithPlayer = 1

# Bots will auto-complete quests for the player when handing in
# 默认：0 (disabled)
AiPlayerbot.SyncQuestForPlayer = 0

# Sync max random bot level with max level of online players
# 默认：0 (disabled)
AiPlayerbot.SyncLevelWithPlayers = 0

# Give free food to random bots
# 默认：1 (enabled)
AiPlayerbot.FreeFood = 1

# 机器人在于训练师对话时自动学习法术 (yes = 只要机器人有钱，就训练所有可用的法术, free = 自动学习免费法术, no = 仅列出法术)
# 仅用于随机机器人
AiPlayerbot.AutoTrainSpells = yes

# 机器人在升级时自动学习职业任务奖励法术
# 仅用于随机机器人
# 默认：0 (disabled)
AiPlayerbot.AutoLearnQuestSpells = 0

